						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_azuresqldb_bulk",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "prd",
							"tableName": "pipeline_config"
						}
					},
					"firstRowOnly": false
				}
			},
